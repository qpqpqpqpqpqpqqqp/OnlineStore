<main>
    <div class="container">
        <header class="d-flex justify-content-center py-3">
            <ul class="nav nav-pills">
                <li class="nav-item"><a href="/seller/profile" class="nav-link">My profile</a></li>
                <li class="nav-item"><a href="/seller/create" class="nav-link active" aria-current="page">Create new</a></li>
<!--                <li class="nav-item"><a href="/seller/stock" class="nav-link">Change Stock</a></li>-->
                <li class="nav-item"><a href="/seller/update" class="nav-link">Update</a></li>
